# API Keys for Sentiment Analysis

# Google Gemini API Key (free tier: 60 requests/minute)
GEMINI_API_KEY=your_gemini_api_key_here

# Alpha Vantage API Key (free tier: 25 requests/day)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# NewsAPI Key (free tier: 100 requests/day)
NEWS_API_KEY=your_newsapi_key_here

# Reddit API credentials (optional, for social sentiment)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# Server Configuration
PORT=3000
NODE_ENV=development

# Cache Configuration
CACHE_TTL=1800
